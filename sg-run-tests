#!/usr/bin/tclsh8.4
# -*- Tcl -*- 
# usage: ./sg-run-tests <flight>

source osstestlib.tcl

readconfig

proc main {} {
    global flight jobinfo builds anyfailed argv

    set-flight
    become-task "$flight $argv"
    
    set tests {}
    set anyfailed 0

    db-open

    set to_run {}

    db-close

    foreach jobitem $to_run {
        lassign $jobitem jobinfo(job) jobinfo(recipe)
        run-job
    }
}

main
